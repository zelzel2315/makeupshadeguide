<!-- <h1 class="landtitle"><%#=link_to "Makeup Shade Guide", root_path %></h1> -->

  <div id="signinTitle"></div>
    <div class="row">
      <div class="large-6 columns" id="signinform">
        <h1 class = "landTitle">Login</h1>
        <%= form_for @user, {url: session_path} do |f| %>
          <div class="form-group">
          <%= f.text_field :email, placeholder: 'Email', class: 'form-control' %><br>
          </div>
          <div class="form-group">
          <%= f.password_field :password, placeholder: 'Password', class: 'form-control' %><br>
          </div>
            <br>
          <%= f.submit 'Login',:class => 'small button radius' %>
        <% end %>
      </div>    

      <div class="large-6 columns" id="poster2"></div>
    </div>
  </div>
  <%#= link_to 'Log Out', session_path, method: :delete %>